#, no-wrap
msgid "B<--cols>I<\\ n>"
msgstr "B<--cols>I<\\ n>"

#. type: Plain text
#: ../man/ps.1:563 ../man/ps.1:566 ../man/ps.1:714
msgid "Set screen width."
msgstr "设置屏幕宽度。"

#. type: TP
#: ../man/ps.1:563
